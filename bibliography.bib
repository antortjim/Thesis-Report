pages = {403--420},
publisher = {Humana Press},
title = {{Purification and Fractionation of Membranes for Proteomic Analyses}},
url = {https://doi.org/10.1385/1-59745-003-0:403},
year = {2006}
}


@article{Meier2015,
abstract = {In liquid chromatography-mass spectrometry (LC-MS)-based proteomics, many precursors elute from the column simultaneously. In data-dependent analyses, these precursors are fragmented one at a time, whereas the others are discarded entirely. Here we employ trapped ion mobility spectrometry (TIMS) on an orthogonal quadrupole time-of-flight (QTOF) mass spectrometer to remove this limitation. In TIMS, all precursor ions are accumulated in parallel and released sequentially as a function of their ion mobility. Instead of selecting a single precursor mass with the quadrupole mass filter, we here implement synchronized scans in which the quadrupole is mass positioned with sub-millisecond switching times at the m/z values of appropriate precursors, such as those derived from a topN precursor list. We demonstrate serial selection and fragmentation of multiple precursors in single 50 ms TIMS scans. Parallel accumulation–serial fragmentation (PASEF) enables hundreds of MS/MS events per second at full sensitivity. Mo...},
author = {Meier, Florian and Beck, Scarlet and Grassl, Niklas and Lubeck, Markus and Park, Melvin A. and Raether, Oliver and Mann, Matthias},
doi = {10.1021/acs.jproteome.5b00932},
issn = {1535-3893},
journal = {Journal of Proteome Research},
keywords = {MS/MS,TIMS,high resolution,ion mobility,multiplexing,peptide sequencing,proteomics,time-of-flight},
mendeley-groups = {TFM},
month = {dec},
number = {12},
pages = {5378--5387},
publisher = {American Chemical Society},
title = {{Parallel Accumulation–Serial Fragmentation (PASEF): Multiplying Sequencing Speed and Sensitivity by Synchronized Scans in a Trapped Ion Mobility Device}},
url = {http://pubs.acs.org/doi/10.1021/acs.jproteome.5b00932},
volume = {14},
year = {2015}
}

